class Solution {
public:
    int numTrees(int n) {
        if(n == 1) return 1;
        vector<int> res;
        res.push_back(1);
        res.push_back(1);
        
        for(int i = 2; i <= n; i++) {
            int ans = 0;
            for(int j = i - 1; j >= 0; j--) {
                ans += (res[j] * res[i - j - 1]);
            }
            res.push_back(ans);
        }
        
        return res[n];
    }
};