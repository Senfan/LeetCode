class Solution {
public:
    int titleToNumber(string s) {
        if(s == "") return 0;
        int sLen = s.size();
        
        int res = 0;
        int bitNum = 0;
        for(int i = 0; i < sLen; i++) {
            res = res * 26 + s[i] - 'A' + 1;
        }
        
        return res;
    }
};