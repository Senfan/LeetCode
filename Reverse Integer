O(len) 14ms

class Solution {
public:
    int reverse(int x) {
        int t;
        long long  re = 0;
        int sign = x < 0 ? -1 : 1;


        long long lx = x;
        lx = abs(lx);


        while(lx) {
            t = lx % 10;
            re = re * 10 + t;
            lx /= 10;
        }
        if( re > INT_MAX ) return 0;
        return re * sign;
    }
};
